{
    "type": "login-theme",
    "name": "cPanel Standard Login Theme",
    "description": "This theme is deployed with the cPanel product.",
    "disclosure": "If you create a custom login theme for the product, you will have to assume maintenance responsibility for that theme since cPanel does not make updates to customer created login themes.",
    "apps": {
        "shared": {
            "shared-component": true,
            "version" : "2.0",
            "cpanel-version": "11.32+",
            "manifest": [
                ".requirejs",
                "preview.png",
                "favicon.ico",
                "style_v2.less",
                "style_v2.css",
                "style_v2_optimized.css",
                "fonts/open_sans",
                "images/cpanel-logo-tiny-white.png",
                "images/cpanel-logo-tiny.png",
                "images/cpanel-logo.png",
                "images/default-avatar.png",
                "images/icon-password.png",
                "images/icon-token.png",
                "images/icon-username.png",
                "images/locale-map.png",
                "images/login-error-close.png",
                "images/login-whisp.png",
                "images/logo.png",
                "images/notice-error.png",
                "images/notice-info.png",
                "images/notice-success.png",
                "images/or-separator-line.png",
                "images/security-policy-error.png",
                "images/security-policy-success.png",
                "images/warning.png",
                "images/webmail.png",
                "images/webmail-logo.svg",
                "images/whm-white.png",
                "images/whm-logo_white.svg",
                "images/whm.png"
            ]
        },

        "login": {
            "version" : "2.0",
            "cpanel-version": "11.32+",
            "code": "base/unprotected/login.cgi",
            "supports-legacy": true,
            "manifest": [
                "templates/login.tmpl",
                "js/login.js",
                "js/submit_post.js",
                "js-min/cptimezone_optimized.js",
                "js-min/jstz.min.js",
                "js-min/login.js",
                "js-min/submit_post.js"
            ]
        },
        "errors": {
            "version" : "2.0",
            "cpanel-version": "11.32+",
            "last-updated-version": "11.52",
            "code": "base/unprotected/login.cgi",
            "supports-legacy": false,
            "manifest": [
                "main.tmpl",
                "templates/_error_wrapper.tmpl",
                "templates/access_denied.tmpl",
                "templates/error400.tmpl",
                "templates/error502.tmpl",
                "templates/error503.tmpl",
                "templates/external_auth.tmpl",
                "templates/external_auth_header.tmpl",
                "templates/fourohfour.tmpl",
                "templates/generic_error.tmpl",
                "templates/passthrough.tmpl",
                "templates/referrer_denied.tmpl",
                "templates/token_denied.tmpl"
            ]
        },
        "resetpass": {
            "version": "3.0",
            "cpanel-version": "11.56",
            "breaking-change-version": "11.56",
            "supports-legacy": false,
            "code": "base/unprotected/resetpass.cgi",
            "manifest": [
                "resetpass.tmpl",
                "images/icon-envelope.svg",
                "images/icon-envelope.png",
                "templates/disabled.tmpl",
                "templates/resetpass_set_password.tmpl",
                "templates/resetpass_confirmation.tmpl",
                "templates/resetpass_failure.tmpl",
                "templates/resetpass_no_user.tmpl",
                "templates/resetpass_puzzle.tmpl",
                "templates/resetpass_success.tmpl",
                "templates/resetpass_suspended.tmpl",
                "resetpass.devel.js",
                "resetpass.dist.js",
                "resetpass.dist.min.js",
                "resetpass.cmb.js",
                "resetpass.cmb.min.js",
                "resetpass.js",
                "resetpass.less",
                "controllers/puzzleController.js",
                "controllers/setPasswordController.js"
            ]
        },
        "invitation": {
            "version": "3.0",
            "cpanel-version": "11.58",
            "breaking-change-version": "11.58",
            "supports-legacy": false,
            "code": "base/unprotected/resetpass.cgi",
            "manifest": [
                "invitation.devel.js",
                "invitation.dist.js",
                "invitation.dist.min.js",
                "invitation.js",
                "invitation.cmb.js",
                "invitation.cmb.min.js",
                "templates/invitation_failure.tmpl",
                "templates/invitation_set_password.tmpl",
                "templates/invitation_success.tmpl"
            ],
            "depends": [
                "resetpass.tmpl",
                "resetpass.less",
                "controllers/puzzleController.js",
                "controllers/setPasswordController.js"
            ]
        },
        "security-policy": {
            "version": "2.0",
            "cpanel-version": "11.32+",
            "supports-legacy": true,
            "manifest": [
                "securitypolicy_footer.html.tmpl",
                "securitypolicy_header.html.tmpl"
            ]
        },
        "broken": {
            "version": "2.0",
            "cpanel-version": "11.32+",
            "manifest": [
                "broken.css",
                "broken.gif",
                "broken.html"
            ]
        },
        "cpanel-legacy": {
            "fallback": true,
            "version": "1.0",
            "cpanel-version": "",
            "note": "These are sym-links from cpanel_legacy to maintain some limited backward compatibility.",
            "manifest": [
                "footer.html",
                "footer_webmaild.html",
                "footer_whostmgrd.html",
                "header.html",
                "header_webmaild.html",
                "header_whostmgrd.html",
                "images/accept.gif",
                "images/bg.gif",
                "images/bg.jpg",
                "images/button-bg-over.jpg",
                "images/button-bg.jpg",
                "images/cp-logo.svg",
                "images/cp-logo_white.svg",
                "images/cpanel-logo.svg",
                "images/cpanel_login_sprite.jpg",
                "images/log_01.jpg",
                "images/log_01_webmail.jpg",
                "images/log_01_whm.jpg",
                "images/log_02.jpg",
                "images/log_02b.jpg",
                "images/log_03.jpg",
                "images/log_04.jpg",
                "images/log_1_webmail.jpg",
                "images/log_2.jpg",
                "images/mid.jpg",
                "images/reject.gif",
                "images/top-logo.gif",
                "images/webmail_login_sprite.jpg",
                "images/whm_login_sprite.jpg",
                "login_cpaneld.html",
                "login_webmaild.html",
                "login_whostmgrd.html",
                "policy.css",
                "resetpass_header_cpaneld.html",
                "resetpass_reset_cpaneld.html",
                "resetpass_resetform_cpaneld.html",
                "resetpass_resetform_nouser_cpaneld.html",
                "resetpass_resetformheader_cpaneld.html",
                "resetpass_suspended_cpaneld.html",
                "securitypolicy_footer.html",
                "securitypolicy_header.html",
                "style.css",
                "style_optimized.css"
            ]
        }
    }
}
